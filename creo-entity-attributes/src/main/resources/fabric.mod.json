{
  "schemaVersion": 1,
  "id": "creo-entity-attributes",
  "version": "${version}",
  "name": "Creo Entity Attributes",
  "description": "Many new entity attributes for control over various entity systems.",
  "authors": [
    "creoii"
  ],
  "contact": {
    "homepage": "https://www.creoii.dev/projects/creo-api/creo-entity-attributes",
    "sources": "https://github.com/great-big-world/creo-api/tree/master/creo-entity-attributes",
    "issues": "https://github.com/great-big-world/creo-api/issues"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/creo-entity-attributes/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.creoii.creoapi.api.attribute.CreoEntityAttributes"
    ]
  },
  "accessWidener": "creo-entity-attributes.accesswidener",
  "mixins": [
    "creo-entity-attributes.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${loader_version}"
  },
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ],
      "parent": {
        "id": "creo-api",
        "name": "Creo API",
        "description": "A multi-purpose api for Minecraft mod and datapack development.",
        "icon": "assets/creo-entity-attributes/icon.png",
        "badges": [
          "library"
        ]
      }
    }
  }
}
